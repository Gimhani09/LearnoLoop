{
    "java.project.sourcePaths": ["src/main/java"],
    "java.project.outputPath": "target/classes",
    "java.project.referencedLibraries": [
        "lib/**/*.jar"
    ],
    "java.configuration.updateBuildConfiguration": "automatic",
    "java.import.maven.enabled": true,
    "java.project.explorer.showNonJavaResources": true,
    "java.completion.importOrder": [
        "com.example",
        "java",
        "javax",
        "org",
        ""
    ],    "java.jdt.ls.java.home": "C:\\Program Files\\Java\\jdk-20",
    "java.format.enabled": true,
    "java.compile.nullAnalysis.mode": "automatic",
    "java.project.exportJar.targetPath": "${workspaceFolder}/target/${workspaceFolderBasename}.jar",
    "java.silentNotification": false,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/target": false
    },
    "java.debug.settings.hotCodeReplace": "auto",
    "java.configuration.runtimes": [
        {
            "name": "JavaSE-17",
            "path": "C:\\Program Files\\Java\\jdk-20",
            "default": true
        }
    ],
    "maven.executable.preferMavenWrapper": true
}
